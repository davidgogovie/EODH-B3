{
  "name": "EODH-B",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "json-server": "json-server --watch data/db.json --host 0.0.0.0 --port 8000",
    "server": "node server.js",
    "client": "expo start --offline",
    "client2": "expo start",
    "client3": "expo start --tunnel",
    "myandroid": "expo start --offline --android",
    "myios": "expo start --offline --ios",
    "myweb": "expo start --offline --web",
    "client-online": "expo start",
    "myandroid-online": "expo start --android",
    "myios-online": "expo start --ios",
    "myweb-online": "expo start --web",
    "all": "npm-run-all --parallel json-server server client",
    "all-android": "npm-run-all --parallel json-server server myandroid",
    "all-ios": "npm-run-all --parallel json-server server myios",
    "all-web": "npm-run-all --parallel json-server server myweb",
    "all-online": "npm-run-all --parallel json-server server client-online",
    "all-android-online": "npm-run-all --parallel json-server server myandroid-online",
    "all-ios-online": "npm-run-all --parallel json-server server myios-online",
    "all-web-online": "npm-run-all --parallel json-server server myweb-online",
    "mystart": "npm run all-web",
    "start": "npm run all-android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "web-start": "npm run all-android"
  },
  "dependencies": {
    "axios": "^1.3.1",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "formidable": "^2.1.1",
    "json-serve": "^0.1.0",
    "json-server": "^0.17.1",
    "mongoose": "^6.9.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "pdf-parse": "^1.1.1",
    "uuid": "^9.0.0"
  },
  "private": true
}
